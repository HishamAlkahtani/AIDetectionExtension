{
  "manifest_version": 2,
  "name": "ai-detector-extension",
  "version": "1.0",

  "description": "Detects and flags AI generated content.",

  "permissions": [
    "webRequest"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.google.com/search*", "<all_urls>"],
      "js": ["index.js"]
    }
  ]
}
